<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ 
                   ..\..\FrameXML\UI.xsd">

  <Frame name="HuntedListHelper_Frame" parent="UIParent" toplevel="true" enableMouse="true" movable="true" hidden="false">
    <Size>
      <AbsDimension x="400" y="400"/>
    </Size>
    <Anchors>
      <Anchor point="CENTER"/>
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="11" right="12" top="12" bottom="11"/>
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32"/>
      </TileSize>
      <EdgeSize>
        <AbsValue val="32"/>
      </EdgeSize>
    </Backdrop>
    <Frames>
      <ScrollFrame>
        <Size>
          <AbsDimension x="368" y="368"/>
        </Size>
        <Anchors>
          <Anchor point="CENTER"/>
        </Anchors>
        <ScrollChild>
          <Frame>
            <Size>
              <RelDimension x="1.0" y="1.0"/>
            </Size>
            <Anchors>
              <Anchor point="CENTER"/>
            </Anchors>
            <Frames>
              <EditBox name="HuntedListHelper_EditBox" font="GameFontNormal" letters="0" multiLine="true" autoFocus="false">
                <Anchors>
                  <Anchor point="TOPLEFT">
                    <Offset>
                      <AbsDimension x="0" y="0"/>
                    </Offset>
                  </Anchor>
                  <Anchor point="BOTTOMRIGHT">
                    <Offset>
                      <AbsDimension x="0" y="0"/>
                    </Offset>
                  </Anchor>
                </Anchors>
              </EditBox>
            </Frames>
          </Frame>
        </ScrollChild>
      </ScrollFrame>
      <Button name="$parent_ButtonClose" inherits="OptionsButtonTemplate" text="Close">
        <Anchors>
          <Anchor point="BOTTOMRIGHT">
            <Offset x="-12" y="-20"/>
          </Anchor> 
        </Anchors>
        <Scripts>
          <OnClick> self:GetParent():Hide(); </OnClick>
        </Scripts>
      </Button>
      <Button name="$parent_ButtonUpdate" inherits="OptionsButtonTemplate" text="Update">
        <Anchors>
          <Anchor point="BOTTOMLEFT">
            <Offset x="12" y="-20"/>
          </Anchor> 
        </Anchors>
      </Button>
    </Frames>
  </Frame>
</Ui>
